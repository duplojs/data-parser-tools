exports[`string > applies string checkers 1`] = `
"{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/StringSchema",
    "definitions": {
        "StringSchema": {
            "type": "string",
            "minLength": 2,
            "pattern": "^[a-z]+$"
        }
    }
}"
`;

