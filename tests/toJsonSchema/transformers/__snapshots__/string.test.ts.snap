exports[`string > handles email and ignores unknown checker 1`] = `
"{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/StringEmailSchema",
    "definitions": {
        "StringEmailSchema": {
            "type": "string",
            "format": "email",
            "pattern": "^(?!\\\\.)(?!.*\\\\.\\\\.)([A-Za-z0-9_'+\\\\-.]*)[A-Za-z0-9_+-]@([A-Za-z0-9][A-Za-z0-9-]*\\\\.)+[A-Za-z]{2,}$"
        }
    }
}"
`;

exports[`string > handles url and max 1`] = `
